--
-- Table structure for table `wp_rg_authorizenet_transaction`
--

CREATE TABLE `wp_rg_authorizenet_transaction` (
  `id` mediumint(8) unsigned NOT NULL AUTO_INCREMENT,
  `entry_id` int(10) unsigned NOT NULL,
  `transaction_type` varchar(15) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `subscription_id` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `transaction_id` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `is_renewal` tinyint(1) NOT NULL DEFAULT '0',
  `amount` decimal(19,2) DEFAULT NULL,
  `date_created` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `txn_id` (`transaction_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

